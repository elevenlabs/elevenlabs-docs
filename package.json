{
  "name": "elevenlabs-docs",
  "private": true,
  "version": "1.0.0",
  "description": "ElevenLabs Documentation",
  "main": "index.js",
  "scripts": {
    "dev": "fern docs dev",
    "format": "prettier \"**/*.{css,js,ts,tsx,md,mdx,yml,yaml,json}\" --write",
    "format:check": "prettier \"**/*.{css,js,ts,tsx,md,mdx,yml,yaml,json}\" --check",
    "fern:upgrade": "fern upgrade",
    "fern:check": "NODE_NO_WARNINGS=1 fern check",
    "fern:preview": "fern generate --docs --preview",
    "sdk:preview-python": "fern generate --group python-sdk --preview --api api",
    "sdk:preview-typescript": "fern generate --group typescript-sdk --preview --api api",
    "sdk:preview": "pnpm run sdk:preview-python && pnpm run sdk:preview-typescript",
    "openapi:latest": "curl -o ./fern/apis/api/openapi.json https://api.elevenlabs.io/openapi.json"
  },
  "keywords": [
    "elevenlabs",
    "documentation",
    "api",
    "docs"
  ],
  "author": "elevenlabs",
  "license": "MIT",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "fern-api": "^0.46.10",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9"
  }
}
